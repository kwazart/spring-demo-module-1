<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.polozov.springDemo.dao.CsvDaoImpl" name="parserUtil">
        <constructor-arg name="fileName" value="/questions.csv"/>
        <constructor-arg name="converter" ref="questionConverter"/>
    </bean>

    <bean id="questionConverter" class="com.polozov.springDemo.util.QuestionConverterUtil"/>
    <bean id="questionPrinter" class="com.polozov.springDemo.view.QuestionPrinter"/>
</beans>